function PART(Par,name,size,color,mat,ref,tra)
local part=Instance.new("Part")
part.Parent=Par
part.Name=name
part.Size=size
part.CanCollide=false
part.Anchored=false
part.BrickColor=BrickColor.new(color)
part.Material=mat
part.Reflectance=ref
part.Transparency=tra
--Position=Torso.Position}
part.TopSurface,part.BottomSurface,part.LeftSurface,part.RightSurface,part.FrontSurface,PartpartBackSurface=10,10,10,10,10,10
part:BreakJoints()
return part 
end

function MESH(Par,name,scale,mtype,id)
local mesh=Instance.new("SpecialMesh")
mesh.Parent=Par
mesh.Name=name
mesh.Scale=scale
mesh.MeshType=mtype
if id~="" then
mesh.MeshId="rbxassetid://"..id
end
return mesh
end

function WELD(Par,name,p0,p1,c0,c1)
local weld=Instance.new("Weld")
weld.Parent=Par
weld.Name=name
weld.Part0=p0
weld.Part1=p1
weld.C0=c0
weld.C1=c1
return weld
end

function SOUND(Par,id,vol,pit)
local sound=Instance.new("Sound")
sound.Parent=Par
sound.SoundId="rbxassetid://"..id
sound.Volume=vol
sound.PlaybackSpeed=pit
sound.MaxDistance=66666666
sound:Play()
game:GetService("Debris"):AddItem(sound,10)
return sound
end

mode=Instance.new("Model")
mode.Parent=char
mode.Name ="WModel"
--Stuffs

--Stuffs

New = function(Object, Parent, Name, Data)
	local Object = Instance.new(Object)
	for Index, Value in pairs(Data or {}) do
		Object[Index] = Value
	end
	Object.Parent = Parent
	Object.Name = Name
	return Object
end

--Upper-head Shadow by IgnorantRojo
FHead = New("Part",char,"Derpz's Upper Shadow",{CanCollide = false,BrickColor = BrickColor.new("Really black"),Size = Vector3.new(1.20000005, 0.600000024, 1),CFrame = CFrame.new(68.5999985, 0.700013041, 9.89999962, 1, 0, 0, 0, 1, 0, 0, 0, 1),Color = Color3.new(0.0666667, 0.0666667, 0.0666667),})
Mesh = New("SpecialMesh",FHead,"Mesh",{Scale = Vector3.new(1.25999999, 1.5, 1.25999999),})
Weld = New("Weld",FHead,"mot",{Part0 = FHead,Part1 = char.Head,C1 = CFrame.new(0, 0.200000048, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1),})
FHead.CanCollide = false
FHead.Transparency = 0.2


