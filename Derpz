function ultipoweredblast()
	attack = true
	hum.WalkSpeed = 0
	CFuncs["Sound"].Create("rbxassetid://935829907", workspace, 1.5, 1)
	bosschatfunc("RRRRRRRRAAAAAAAAAAAAAAAAAAAA!!",MAINRUINCOLOR.Color,2.5)
	for i = 0, 9, 0.1 do
		swait()
RH.C0=clerp(RH.C0,cf(1,-1 - 0.1 * math.cos(sine / 20),0)*angles(math.rad(0),math.rad(90),math.rad(0))*angles(math.rad(-3),math.rad(-5.5),math.rad(20)),.1)
