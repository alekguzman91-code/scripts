sphereMK(5,1.5,"Add",ba.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),15,15,25,-0.15,BrickColor.new("Navy blue"),0)
sphereMK(5,1.5,"Add",ba.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),15,15,25,-0.15,MAINRUINCOLOR,0)
MagniDamage(ba, 30, 5,25, 0, "Normal")
end
a2:Destroy()
ba:Destroy()
orb:Destroy()
orbe:Destroy()
attack = false
end
function InsaneGroundStrike()
attack = true
