		  	local Alpha = .2
  	RootJoint.C0 = RootJoint.C0:lerp(CF(-0.1,0,-0.1)*ANGLES(RAD(-90),RAD(0),RAD(116.4)),Alpha)
  	LeftShoulder.C0 = LeftShoulder.C0:lerp(CF(-0.8,0.2,-0.4)*ANGLES(RAD(-176.8),RAD(-15.5),RAD(107.6)),Alpha)
