function axes()
	ATTACK = true
	for i = 0,20 do
	Swait()
	  	local Alpha = .1
  	RootJoint.C0 = RootJoint.C0:lerp(CF(0,0,0)*ANGLES(RAD(-90),RAD(0),RAD(-146)),Alpha)
