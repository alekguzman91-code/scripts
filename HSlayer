  	RightHip.C0 = RightHip.C0:lerp(CF(0.9,-1,0.2)*ANGLES(RAD(0),RAD(58.5),RAD(0)),Alpha)	
	end
	for i = 0,20 do
		local Alpha = .1
  	RootJoint.C0 = RootJoint.C0:lerp(CF(-0.1,0,0)*ANGLES(RAD(-90),RAD(0),RAD(158)),Alpha)
